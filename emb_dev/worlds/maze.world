<?xml version="1.0" ?>
<sdf version="1.6">
  <world name="maze_world">
    <include>
      <uri>model://ground_plane</uri>
    </include>

    <!-- Define the maze as a set of walls -->
    <include>
      <uri>model://maze_wall</uri>
      <pose>0 0 0 0 0 0</pose>
      <static>true</static>
      <name>maze_walls</name>
    </include>

    <!-- Add a light to the scene -->
    <include>
      <uri>model://sun</uri>
      <pose>0 0 10 0 0 0</pose>
      <name>sun</name>
    </include>

    <!-- Add a camera to the scene -->
    <include>
      <uri>model://camera</uri>
      <pose>0 -5 2 0 0 0</pose>
      <name>camera</name>
    </include>
  </world>
</sdf>
